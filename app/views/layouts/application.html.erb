<!DOCTYPE html>
<html>
  <head>
    <title>Bike Fall River</title>
    <%= csrf_meta_tags %>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>

  <body>
    <nav id="custom-bootstrap-menu" class="navbar navbar-default navbar-fixed-top" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header"><a class="navbar-brand" href="#">Bike Fall River</a>
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-menubuilder"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
          </button>
      </div>
      <div class="collapse navbar-collapse navbar-menubuilder">
        <ul class="nav navbar-nav navbar-right">
            <li><a href="/">Home</a>
            </li>
            <li><%= link_to "Products", products_path %>
            </li>
            <li><%= link_to "About", static_pages_about_path %>
            </li>
            <li><%= link_to "Contact", static_pages_contact_path %>
            </li>
          </ul>
        </div>
      </div>
    </nav>




    






<!-- Yield -->
          <%= yield %>
<!-- End Yield -->





    
    <footer class="footer">
      <div class="container">
        <p>&copy; 2016 Bike Fall River</p>
      </div>
    </footer>
  </body>
</html>
